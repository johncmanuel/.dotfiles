{
	"workbench.startupEditor": "newUntitledFile",
	"atomKeymap.promptV3Features": true,
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.formatOnPaste": true,
	"terminal.integrated.shell.windows": "C:\\windows\\System32\\cmd.exe",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"diffEditor.ignoreTrimWhitespace": false,
	"vsicons.associations.files": [
		{
			"icon": "rpy",
			"extensions": ["rpy", "rpyc"],
			"format": "ico"
		}
	],
	"editor.fontFamily": "Fira Code Retina",
	"editor.fontWeight": "400",
	"editor.fontLigatures": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"liveshare.authenticationProvider": "GitHub",
	"liveshare.connectionMode": "relay",
	"vsicons.dontShowNewVersionMessage": true,
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"terminal.integrated.defaultProfile.windows": "PowerShell",
	"bracket-pair-colorizer-2.depreciation-notice": false,
	"[python]": {
		"editor.formatOnType": true
	},
	"terminal.integrated.enableMultiLinePasteWarning": false,
	"vsicons.presets.hideFolders": true,
	"[ini]": {},
	"terminal.integrated.fontFamily": "monospace",
	"editor.mouseWheelZoom": true,
	"files.associations": {
		"*.ini": "plaintext"
	},
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"svelte.enable-ts-plugin": true,
	"terminal.integrated.mouseWheelScrollSensitivity": 0.4,
	"vscode-pets.theme": "forest",
	"playwright.reuseBrowser": false,
	"go.toolsManagement.autoUpdate": true,
	"playwright.showTrace": false,
	"dotnetAcquisitionExtension.enableTelemetry": false,
	"telemetry.telemetryLevel": "off",
	"github.copilot.enable": {
		"*": true,
		"plaintext": false,
		"markdown": false,
		"scminput": false,
		"asm-intel-x86-generic": false,
		"csharp": true,
		"typescript": true
	},
	"workbench.colorTheme": "One Hunter Flexoki Dark",
	"workbench.colorCustomizations": {
		"[One Hunter Flexoki Dark]": {
			"activityBarBadge.background": "#205EA6",
			// "activityBarBadge.background": "#007acc", -> default vscode color
			"activityBarBadge.foreground": "#ffffff",
			"button.background": "#205EA6",
			"button.foreground": "#ffffff",
			"badge.background": "#205EA6",
			"badge.foreground": "#ffffff"
		}
	}
	// "[javascript]": {
	//     "editor.defaultFormatter": "esbenp.prettier-vscode"
	// }
}
